# Complex test with multiple features
let fundamental = f
let third_harmonic = 3f
let fifth_harmonic = 5f

# Create a complex wave (additive synthesis)
let complex_wave = 0.5 * fundamental + 0.3 * third_harmonic + 0.2 * fifth_harmonic

# Create another wave with different harmonics
let other_wave = 2f + 4f

# Create a morphing segment
segment(complex_wave, other_wave, 20)