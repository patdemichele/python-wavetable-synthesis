# Test concatenation with different time segments
let fundamental = f
let second_harmonic = 2f

# First segment: morph from f to 2f over 8 time units
let first_part = segment(fundamental, second_harmonic, 8)

# Second segment: morph from 2f back to f over 3 time units
let second_part = segment(second_harmonic, fundamental, 3)

# Concatenate them
cat(first_part, second_part)